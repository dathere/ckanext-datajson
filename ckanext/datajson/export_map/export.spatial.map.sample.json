    },
    "accrualPeriodicity": {
      "extra": true,
      "field": "frequency_of_data_collection"
    },
    "license": {
      "extra": true,
      "field": "fees"
    },
    "landingPage": {
      "extra": true,
      "field": "where_to_apply"
    },
    "language": {
      "type": "array",
      "extra": true,
      "field": "language",
      "split": ","
    },
    "contactPoint": {
      "wrapper": "build_contact_point",
      "map": {
        "fn": {
          "extra": true,
          "field": "program"
        },
        "hasEmail": {
          "extra": true,
          "field": "url_for_more_info_about_restricted_file"
        }
      }
    },
    "distribution": {
      "wrapper": "generate_distribution",
      "map": {
        "accessURL": {
          "field": "url"
        },
        "mediaType": {
          "field": "format"
        }
      }
    },
    "dataQuality": {
      "extra": true,
      "field": "method_of_data_collection"
    },
    "theme": {
      "type": "array",
      "extra": true,
      "field": "classification",
      "split": ","
    },
    "rights": {
      "extra": true,
      "field": "usage_restrictions"
    },
    "conformsTo": {
      "extra": true,
      "field": "documentation_for_application"
    },
    "describedBy": {
      "extra": true,
      "field": "supporting_documentation"
    },
    "name": {
      "field": "name"
    },
    "owner_org": {
      "field": "owner_org"
    },
    "agency_id": {
      "extra": true,
      "field": "agency_id"
    },
    "agency_datafile_id": {
      "extra": true,
      "field": "agency_datafile_id"
    },
    "alternative_title": {
      "extra": true,
      "field": "alternative_title"
    },
    "authorizer(s)": {
      "type": "array",
      "extra": true,
      "field": "authorizer(s)",
      "split": ","
    },
    "funder/sponsor(s)": {
      "type": "array",
      "extra": true,
      "field": "funder/sponsor(s)",
      "split": ","
    },
    "program": {
      "extra": true,
      "field": "program"
    },
    "preferred_citation": {
      "extra": true,
      "field": "preferred_citation"
    },
    "url_for_general_survey_info": {
      "extra": true,
      "field": "url_for_general_survey_info"
    },
    "doi": {
      "extra": true,
      "field": "doi"
    },
    "universe": {
      "extra": true,
      "field": "universe"
    },
    "unit_of_observation": {
      "extra": true,
