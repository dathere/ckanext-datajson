    },
    "data_collection_notes": {
      "extra": true,
      "field": "data_collection_notes"
    },
    "number_of_cases": {
      "extra": true,
      "field": "number_of_cases"
    },
    "number_of_variables": {
      "extra": true,
      "field": "number_of_variables"
    },
    "linkage_capabilities": {
      "extra": true,
      "field": "linkage_capabilities"
    },
    "linkage_variables": {
      "type": "array",
      "extra": true,
      "field": "linkage_variables",
      "split": ","
    },
    "public-use_version": {
      "extra": true,
      "field": "public-use_version"
    },
